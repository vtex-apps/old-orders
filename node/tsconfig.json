{
  "compilerOptions": {
    "types": ["node"],
    "typeRoots": ["node_modules/@types", "./typings.d.ts"],
    "target": "es2019",
    "module": "commonjs",
    "sourceMap": false,
    "outDir": "service",
    "allowJs": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true
  },
  "typeAcquisition": {
    "enable": false
  },
  "include": ["./typings.d.ts", "./**/*"],
  "exclude": ["node_modules"]
}
